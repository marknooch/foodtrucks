{
  "Version": "2012-10-17",
  "Statement": [
      {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::${bucket}/*"
      },
      {
      "Sid": "github-actions",
      "Effect": "Allow",
      "Principal": {
          "AWS": "${github-actions-arn"
      },
      "Action": "s3:*",
      "Resource": "arn:aws:s3:::${bucket}/*"
      }
  ]
}